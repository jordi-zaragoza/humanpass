{
  "manifest_version": 3,
  "name": "humanpass",
  "version": "1.0.0",
  "description": "Generate humanpass verification links with one click",
  "permissions": [],
  "host_permissions": [
    "https://humanpass.latent-k.workers.dev/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "humanpass",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
